    "@types/react-dom": "^16.9.8",
    "@types/react-ranger": "^2.0.1",
    "@types/styled-components": "^5.1.17",
    "concurrently": "^5.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "electronmon": "^1.1.2",
    "node-sass": "^7.0.0",
    "node-scss": "^7.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.3.1",
    "react-ranger": "^2.1.0",
    "react-router-dom": "^6.0.2",
