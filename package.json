{
  "name": "easystream",
  "version": "0.1.0",
  "description": "EasyStream, the solution to stream easily.",
  "author": {
    "email": "easystream@epitech.eu",
    "name": "EasyStream Co",
    "url": "https://easystream.com"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.2.3",
    "@mui/styled-engine-sc": "^5.1.0",
    "@mui/styles": "^5.10.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.50",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-ranger": "^2.0.1",
    "@types/styled-components": "^5.1.17",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-renderer": "^2.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-webpack": "^2.8.2",
    "electronmon": "^1.1.2",
    "material-ui-numeric-input": "^3.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.3.1",
    "react-input-number": "^5.0.19",
    "react-numeric-input": "^2.2.3",
    "react-ranger": "^2.1.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "3.4.1",
    "scss": "^0.2.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "styled-components": "^5.3.3",
    "typescript": "^4.7.4",
    "wait-on": "^5.1.0",
    "webpack": "^5.73.0"
  },
  "scripts": {
    "front-dev": "concurrently \"ELECTRON_IS_DEV=1 yarn react-start\" \"wait-on http://localhost:3000 && ELECTRON_IS_DEV=1 yarn electron-start\"",
    "front-with-obs": "concurrently \"ELECTRON_IS_DEV=0 yarn react-start\" \"wait-on http://localhost:3000 && ELECTRON_IS_DEV=0 yarn electron-start\"",
    "serve": "concurrently -k \"cross-env ELECTRON_IS_DEV=1 yarn react-start\" \"wait-on http://localhost:3000 && ELECTRON_IS_DEV=1 electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -wl",
    "build-dist": "yarn build && electron-builder",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "yarn react-build && npm run electron-build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "compile": "electron-webpack",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "eject": "react-scripts eject",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^19.0.7",
    "electron-builder": "^22.10.5",
    "react-error-overlay": "6.0.9"
  },
  "build": {
    "appId": "easystream.client",
    "productName": "EasyStream.Client",
    "copyright": "Copyright @2021-2024 Easystream",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": "AppImage"
    },
    "extraMetadata": {
      "main": "public/electron.js"
    },
    "extends": null
  },
  "postinstall": "electron-builder install-app-deps"
}
