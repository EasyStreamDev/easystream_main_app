{
  "name": "easystream",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.2.3",
    "@mui/styled-engine-sc": "^5.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.50",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-ranger": "^2.0.1",
    "@types/styled-components": "^5.1.17",
    "concurrently": "^5.2.0",
    "electron": "^19.0.7",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "electron-renderer": "^2.0.4",
    "electron-webpack": "^2.8.2",
    "electronmon": "^1.1.2",
    "node-sass": "^7.0.0",
    "node-scss": "^7.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.3.1",
    "react-ranger": "^2.1.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "3.4.1",
    "scss": "^0.2.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "styled-components": "^5.3.3",
    "typescript": "^4.7.4",
    "wait-on": "^5.1.0",
    "webpack": "^5.73.0"
  },
  "scripts": {
    "front": "concurrently \"ELECTRON_IS_DEV=1 yarn react-serve\" \"wait-on http://localhost:3000 && ELECTRON_IS_DEV=1 yarn electron-start\"",
    "start": "concurrently \"ELECTRON_IS_DEV=0 yarn react-serve\" \"wait-on http://localhost:3000 && ELECTRON_IS_DEV=0 yarn electron-start\"",
    "build": "yarn react-build && npm run electron-build",
    "test": "react-scripts test",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "eject": "react-scripts eject",
    "release": "yarn react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "BROWSER=none && yarn react-start"
  },
  "rescripts": [
    "env"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron-main.js",
  "homepage": "./",
  "devDependencies": {
    "@rescripts/cli": "^0.0.16",
    "@rescripts/rescript-env": "^0.0.14"
  }
}
